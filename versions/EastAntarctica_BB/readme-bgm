
## the native (LAEA) shapefile to bgm (note that -as must be first argument, despite the doc)
java -jar /home/shared/svn/bgmeriser/bgmeriser-stripped.jar -as "+proj=laea +lat_0=-63 +lon_0=82 +x_0=2500000 +y_0=2000000 +ellps=WGS84 +units=m +no_defs" EastAntarctica_BB.shp

## the longlat shapefile to bgm
java -jar /home/shared/svn/bgmeriser/bgmeriser-stripped.jar -as 4326  EastAntarctica_BB_ll.shp

